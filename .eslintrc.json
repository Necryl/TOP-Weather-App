{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["eslint:recommended", "webpack", "prettier"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "linebreak-style": ["error", "unix"],
    "quotes": ["error", "double"],
    "semi": ["error", "always"],
    "line-comment-position": "off",
    "no-param-reassign": "off"
  }
}
